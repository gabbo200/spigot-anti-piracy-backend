{
  "name": "spigot-anti-piracy-backend",
  "version": "0.2.0",
  "description": "A koajs application to blacklist leaked plugins",
  "main": "app.js",
  "private": true,
  "scripts": {
    "build": "babel app.js -d dist",
    "start": "node dist/app.js",
    "babel-node": "./node_modules/babel-cli/bin/babel-node.js",
    "watch": "nodemon --exec npm run babel-node -- app.js",
    "test": "mocha --compilers js:babel-core/register",
    "posttest": "npm run coverage",
    "coverage": "npm run babel-node -- ./node_modules/babel-istanbul/lib/cli.js cover _mocha"
  },
  "engines": {
    "node": ">=4.2"
  },
  "author": "xGhOsTkiLLeRx",
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "koa": "^2.0.0-alpha.3",
    "koa-bodyparser": "^3.0.0",
    "koa-router": "^7.0.1",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta10",
    "babel-istanbul": "^0.6.0",
    "babel-plugin-transform-async-to-generator": "^6.4.6",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-3": "^6.3.13",
    "codeclimate-test-reporter": "^0.3.1",
    "coveralls": "^2.11.6",
    "cz-customizable": "^2.7.1",
    "eslint": "^1.10.3",
    "mocha": "^2.4.5",
    "nodemon": "^1.8.1",
    "should": "^8.2.1",
    "supertest": "^1.1.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
